{
  "development": {
    "username": "DB_USERNAME",
    "password": "DB_PASSWORD",
    "database": "DB_DATABASE",
    "dialect": "postgres"
  },
  "production": {
    "use_env_variable": "DATABASE_URL",
    "dialect": "postgres",
    "dialectOptions": {
      "ssl": {
        "require": true,
        "rejectUnauthorized": false
      }
    }
  }
}
